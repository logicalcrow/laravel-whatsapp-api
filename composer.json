{
  "name": "logicalcrow/laravel-whatsapp-api",
  "description": "A Whatsapp Business Cloud API wrapper for Laravel.",
  "keywords": [
    "laravel",
    "whatsapp",
    "facebook",
    "sdk",
    "api"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Logicalcrow\\Whatsapp\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Giovanni Cespedes",
      "email": "giovanni@logicalcrow.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/contracts": "^10|^11",
    "illuminate/support": "^10|^11",
    "illuminate/http": "^10|^11",
    "illuminate/routing": "^10|^11",
    "illuminate/events": "^10|^11",
    "illuminate/notifications": "^10|^11"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Logicalcrow\\Whatsapp\\WhatsappServiceProvider"
      ],
      "aliases": {
        "Whatsapp": "Logicalcrow\\Whatsapp\\Facade\\Whatsapp"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
